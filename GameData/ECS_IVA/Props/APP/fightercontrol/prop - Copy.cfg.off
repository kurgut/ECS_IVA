PROP
{
  name = Seat_Control
  
 MODEL
  {
    model = AirplanePlus/Props/fightercontrol/model
  }
MODULE
{
	name = FSinternalPropRotator // allows for rotating three different objects in an engine or other part. Must be three separate objects or empties.
	pitchObject = pitchobject //rotates around the y axis
	yawObject = yawright //rotates around the x axis
	rollObject = rollobject // rotates around the z axis
	pitchDefaultRotation =  0, 0, 0 //0,0,0 etc can be used to add to the 0,0,0 + p,r,y rotation reset each update
	rollDefaultRotation =  0, 0, 0
	yawDefaultRotation =  -90, 0, 0
	axisMultiplier = 10, 10, 5 // pitch, roll, yaw
}

MODULE
{
	name = FSinternalPropRotator // allows for rotating three different objects in an engine or other part. Must be three separate objects or empties.
	pitchObject = none 
	yawObject = yawleft //rotates around the x axis
	rollObject = none 
	pitchDefaultRotation =  0, 0, 0 //0,0,0 etc can be used to add to the 0,0,0 + p,r,y rotation reset each update
	rollDefaultRotation =  0, 0, 0
	yawDefaultRotation =  -90, 0, 0
	axisMultiplier = 10, -20, -5 // pitch, roll, yaw
}

MODULE
{
	name = FSinternalPropRotator // allows for rotating three different objects in an engine or other part. Must be three separate objects or empties.
	pitchObject = none 
	yawObject = pedalright //rotates around the x axis
	rollObject = none 
	pitchDefaultRotation =  0, 0, 0 //0,0,0 etc can be used to add to the 0,0,0 + p,r,y rotation reset each update
	rollDefaultRotation =  0, 0, 0
	yawDefaultRotation =  0, 0, 0
	axisMultiplier = 10, -20, -10 // pitch, roll, yaw
	brakeMultiplier = 0, 0, -20
}

MODULE
{
	name = FSinternalPropRotator // allows for rotating three different objects in an engine or other part. Must be three separate objects or empties.
	pitchObject = none 
	yawObject = pedalleft //rotates around the x axis
	rollObject = none 
	pitchDefaultRotation =  0, 0, 0 //0,0,0 etc can be used to add to the 0,0,0 + p,r,y rotation reset each update
	rollDefaultRotation =  0, 0, 0
	yawDefaultRotation =  0, 0, 0
	axisMultiplier = 10, -20, 10 // pitch, roll, yaw
	brakeMultiplier = 0, 0, -20
}
}

